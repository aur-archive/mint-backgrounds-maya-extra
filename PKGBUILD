pkgname=mint-backgrounds-maya-extra
pkgver=1.1
pkgrel=1
pkgdesc="Extra desktop backgrounds for Linux Mint 13 Maya"
arch=('any')
url="http://packages.linuxmint.com/pool/main/m/mint-backgrounds-maya-extra"
license=('unknown')
source=("${url}/${pkgname}_${pkgver}.tar.gz")
md5sums=('SKIP')

package() {
  cd "${pkgname}"
  cp -R ./usr "${pkgdir}"

}
